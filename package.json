{
  "name": "@catppuccin/tailwindcss",
  "version": "0.1.1",
  "main": "plugin.js",
  "author": "Catppuccin Org <releases@catppuccin.com>",
  "contributors": [
    "winston <hey@winston.sh>",
    "Adal Zanabria <adal.zanabria@gmail.com>"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "esbuild plugin.ts --bundle --outfile=./dist/plugin.js --platform=node --target=es2019 --format=cjs --external:tailwindcss",
    "release": "standard-version",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts"
  },
  "files": [
    "plugin.js"
  ],
  "bugs": {
    "url": "https://github.com/catppuccin/tailwindcss/issues"
  },
  "homepage": "https://github.com/catppuccin/tailwindcss#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/catppuccin/tailwindcss.git"
  },
  "keywords": [
    "pastel-colors",
    "catppuccin",
    "colors",
    "theme",
    "palette",
    "soothing"
  ],
  "dependencies": {
    "@types/node": "^18.11.9",
    "esbuild": "^0.15.14",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@catppuccin/palette": "0.1.5",
    "@tsconfig/recommended": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0"
  },
  "lint-staged": {
    "*.{js,ts,css,md}": "prettier --write",
    "*.ts": "eslint --cache --fix"
  }
}
